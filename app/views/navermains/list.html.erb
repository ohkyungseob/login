		<script type="text/javascript">
			var isLogin = false;
			var levelName = 'webtoon';

			var artistData = [
				{ artistId: 301243, nickname: '채용택' },
				{ artistId: 314484, nickname: '한가람' },
			];

			var actionRunner;

			WebtoonArtistActionRunner = $Class({
				$init: function (titleId) {
					this._titleId = titleId;
				},

				//관심 웹툰 추가
				addFavoriteWebtoon: function (titleId, event) {
					var width = screen.width / 2 - 800;
					var height = screen.height / 2 - 510;
					var favoriteUrl = null;
					var addUrl =
						'https://' +
						location.hostname +
						'/mypage/favoriteTitleAdd?titleId=' +
						titleId;
					var removeUrl =
						'https://' +
						location.hostname +
						'/mypage/favoriteTitleRemove?titleId=' +
						titleId;
					if ($Element(jindo.$$.getSingle('.book_maker')).hasClass('on')) {
						favoriteUrl = removeUrl;
						nclk_v2(event, 'inf.zzimdel');
					} else {
						favoriteUrl = addUrl;
						nclk_v2(event, 'inf.zzim');
					}

					if (isLogin) {
						var favoritePopup = window.open(
							'',
							'addFavorite',
							'directories=no,toolbar=no,scrollbars=yes,width=370,height=180,top=' +
								height +
								',left=' +
								width
						);
						favoritePopup.location.href = favoriteUrl;
					} else {
						location.href = '/common/login?url=' + encodeURIComponent(location.href);
					}
				},

				artistForArtistTitle: function (element) {
					$Element('author_layer_inner').html('');
					$Element('author_layer').css('border', '');
					$Element('author_layer_inner').css('border', '1px solid #b6b6b6');
					if (artistData.length == 0) {
						var liElement = new $Element('<li>');
						var spanElement = new $Element('<span>');
						spanElement.html('작가의 다른 작품이 없습니다.');
						liElement.append(spanElement);
						$Element('author_layer_inner').append(liElement);
						artistLayer.show(element);
					} else if (artistData.length == 1) {
						location.href = '/artistTitle?artistId=' + artistData[0].artistId;
					} else {
						for (var i = 0; i < artistData.length; i++) {
							var liElement = new $Element('<li>');
							var aElement = new $Element('<a>');
							aElement.attr(
								'href',
								'/artistTitle?artistId=' + artistData[i].artistId
							);
							aElement.html('&lt;' + artistData[i].nickname + '&gt;의 다른작품');
							liElement.append(aElement);
							$Element('author_layer_inner').append(liElement);
							if (i == 9) {
								$Element('author_layer').css('border', '1px solid #b6b6b6');
								$Element('author_layer_inner').css('border', '0');
							}
						}
						artistLayer.show(element);
					}
					return false;
				},
			});

			var bRolling = false;
			var oAdBannerFlicking = null;
			var adBannerInterval = null;

			function loadThisWindow() {
				actionRunner = new WebtoonArtistActionRunner('758037');

				(function (oData) {
					window.__oLikeItData = oData;
					var s = document.createElement('script');
					s.type = 'text/javascript';
					s.charset = 'utf-8';
					s.src =
						oData.domain +
						'/static/js/likeIt.v2.desktop.init.js?v=' +
						new Date().toDateString().replace(/\s/g, '');
					(document.head || document.getElementsByTagName('head')[0]).appendChild(s);
				})({
					serviceId: 'COMIC',
					displayId: 'COMIC',
					contentId: '758037',
					domain: 'https://comic.like.naver.com',
					lang: 'ko',
					viewType: '',

					countCallbackYn: 'Y',
					rparam: encodeURIComponent('deviceCode=PC'),
				});

				if (levelName == 'bestChallenge') {
					function potenupArea() {
						if ($Element('potenupAreaBtn').className() == 'btn_potenup') {
							$Element('potenupAreaBtn').className(
								$Element('potenupAreaBtn').className() + ' on'
							);
						} else {
							$Element('potenupAreaBtn').className('btn_potenup');
						}
					}

					$Fn(potenupArea).attach($Element('potenupAreaBtn'), 'click');

					$Fn(potenupArea).attach($Element('potenupAreaCloseBtn'), 'click');
				}

				if (jQuery('#mflick1')) {
					var $j = jQuery.noConflict();

					if ($j('#mflick1').length > 0) {
						oAdBannerFlicking = new eg.Flicking(jQuery('#mflick1'), {
							duration: 300,
							circular: true,
							defaultIndex: 0,
						});
						jQuery('._adBanner').show();
						rollingAdBanner();

						jQuery(window).scroll(function () {
							var nExposureControlPosition =
								jQuery('#mflick1').offset().top + jQuery('#mflick1').height();
							if (window.scrollY > nExposureControlPosition) {
								window.clearInterval(adBannerInterval);
								bRolling = false;
							} else {
								if (!bRolling) {
									rollingAdBanner();
								}
							}
						});
					}
				}
			}

			function rollingAdBanner() {
				if (!bRolling) {
					adBannerInterval = setInterval(function () {
						var statViewUrl = oAdBannerFlicking.getNextElement().data('url');
						callWebAdBannerView(statViewUrl);
						oAdBannerFlicking.next();
					}, 2000);
					bRolling = true;
				}
			}

			function callWebAdBannerView(sStatsUrl) {
				var self = this;
				var nTimestamp = new Date().getTime();

				jQuery.ajax({
					type: 'GET',
					url: sStatsUrl + '&_dummy=' + nTimestamp,
					async: false,
					timeout: self._nAjaxCallTimeout,
				});
			}

			function callWebAdBannerClick(sStatsUrl, sTargetUrl) {
				var self = this;
				var nTimestamp = new Date().getTime();

				jQuery.ajax({
					type: 'GET',
					url: sStatsUrl + '&_dummy=' + nTimestamp,
					async: false,
					timeout: self._nAjaxCallTimeout,
					complete: function () {
						window.open(sTargetUrl, '_blank');
					},
				});
			}

			$Fn(loadThisWindow).attach(window, 'load');

			/**
			 * 시리즈로 이동 가능한지 확인해서 이동
			 *
			 * @params nbooksId {String} 북스 상품 ID
			 */
			function moveBookstore(nbooksId) {
				var result;

				jQuery.ajax({
					method: 'GET',
					url: '/api/checkTitleInSeries?nbooksId=' + nbooksId,
					async: false,
					success: function (res) {
						result = res.isExist;
					},
				});

				var oInterval = setInterval(function () {
					if (result) {
						window.open(
							'https://series.naver.com/comic/detail.series?originalProductId=' +
								nbooksId,
							'_blank'
						);
					} else {
						alert('시리즈에 상품을 등록 중입니다. 잠시만 기다려주세요.');
					}
					clearInterval(oInterval);
					oInterval = null;
				}, 200);
			}
		</script>

