						jQuery('#deleteFavorite').click(function () {
							var checkedCount = 0;
							var ids = jQuery('._titleIds');
							var titleIds = [];

							for (i = 0; i < ids.length; i++) {
								if (ids[i].checked) {
									checkedCount = checkedCount + 1;
									titleIds.push(ids[i].value);
								}
							}

							if (checkedCount == 0) {
								alert('삭제할 작품을 선택해 주세요');
								return false;
							}

							jQuery.ajaxSettings.traditional = true;
							jQuery.ajax({
								method: 'POST',
								url: '/mypage/favoriteDelete',
								async: false,
								timeout: 3000,
								data: { titleIds: titleIds },
								success: function (result) {
									var jsonResult = JSON.parse(result);
									alert(jsonResult.alertMessage);

									if (jsonResult.status == 'success') {
										location.reload(true);
									}
								},
								error: function () {
									alert('요청이 정상적으로 처리되지 않았습니다.');
								},
